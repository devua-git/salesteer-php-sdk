{
    "name": "devua/salesteer-php-sdk",
    "description": "Salesteer PHP Library",
    "keywords": [
        "salesteer",
        "crm",
        "api"
    ],
    "homepage": "https://salesteer.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Salesteer and contributors",
            "homepage": "https://github.com/devua-git/salesteer-php-sdk/contributors"
        }
    ],
    "require": {
        "php": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.9",
        "php-http/discovery": "^1.19",
        "psr/log": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Salesteer\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "extra": {},
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}